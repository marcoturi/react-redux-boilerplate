{
  "name": "react-redux-boilerplate",
  "version": "1.4.12",
  "packageManager": "yarn@4.5.3",
  "author": "Marco Turi <marco.turi@hotmail.it>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "license": "MIT",
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.example', '.env', fs.constants.COPYFILE_EXCL)\"",
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "e2e": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js --retry 5 --parallel 5",
    "e2e:local": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js",
    "e2e:debug": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js --tags '@only' --fail-fast",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "type:check": "tsc --noEmit",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release",
    "outdated": "yarn upgrade-interactive"
  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=4.0.0"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/themes": "3.1.6",
    "@reduxjs/toolkit": "2.5.0",
    "@sentry/react": "8.47.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.469.0",
    "ramda": "0.30.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "4.1.2",
    "react-redux": "9.2.0",
    "react-router": "7.1.0",
    "react-router-dom": "7.1.0",
    "tailwind-merge": "2.5.5",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@cucumber/cucumber": "11.1.1",
    "@cucumber/html-formatter": "21.7.0",
    "@cucumber/messages": "27.0.2",
    "@cucumber/pretty-formatter": "1.0.1",
    "@eslint/compat": "1.2.4",
    "@eslint/js": "9.17.0",
    "@mswjs/data": "0.16.2",
    "@playwright/test": "1.49.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "5.2.0",
    "@types/node": "22.10.2",
    "@types/ramda": "0.30.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/utils": "8.18.2",
    "@vitejs/plugin-react-swc": "3.7.2",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/eslint-plugin": "1.1.20",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "7.2.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-unicorn": "56.0.1",
    "fs-extra": "11.2.0",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.11",
    "msw": "2.7.0",
    "pinst": "3.0.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "semantic-release": "24.2.0",
    "tailwindcss": "3.4.17",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2",
    "vite": "6.0.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
