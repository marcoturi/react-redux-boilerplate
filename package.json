{
  "name": "react-redux-boilerplate",
  "version": "1.3.116",
  "author": "Marco Turi <marco.turi@hotmail.it>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "packageManager": "yarn@4.5.0",
  "license": "MIT",
  "scripts": {
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.example', '.env', fs.constants.COPYFILE_EXCL)\"",
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "e2e": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js --retry 5 --parallel 5",
    "e2e:local": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js",
    "e2e:debug": "TS_NODE_PROJECT=./e2e/tsconfig.e2e.json SERVER_URL=http://localhost:5173 cucumber-js --tags '@only' --fail-fast",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "type:check": "tsc --noEmit",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release",
    "outdated": "yarn upgrade-interactive"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=4.0.0"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-navigation-menu": "1.2.0",
    "@radix-ui/react-select": "2.1.1",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-switch": "1.1.0",
    "@radix-ui/themes": "3.1.3",
    "@reduxjs/toolkit": "2.2.7",
    "@sentry/react": "8.30.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.445.0",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-redux": "9.1.2",
    "react-router": "6.26.2",
    "react-router-dom": "6.26.2",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "web-vitals": "4.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@cucumber/cucumber": "10.9.0",
    "@cucumber/html-formatter": "21.7.0",
    "@cucumber/messages": "25.0.1",
    "@cucumber/pretty-formatter": "1.0.1",
    "@mswjs/data": "0.16.2",
    "@playwright/test": "1.47.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.3.5",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "22.5.5",
    "@types/ramda": "0.30.2",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "2.1.1",
    "autoprefixer": "10.4.20",
    "cssnano": "7.0.6",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "7.1.1",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.36.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.12",
    "eslint-plugin-unicorn": "54.0.0",
    "eslint-plugin-vitest": "0.5.4",
    "fs-extra": "11.2.0",
    "husky": "9.1.6",
    "jsdom": "24.1.3",
    "lint-staged": "15.2.10",
    "msw": "2.4.9",
    "pinst": "3.0.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "semantic-release": "24.1.1",
    "tailwindcss": "3.4.12",
    "ts-node": "10.9.2",
    "typescript": "5.6.2",
    "vite": "5.3.6",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.1.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
